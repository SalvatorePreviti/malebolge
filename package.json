{
  "name": "@malebolge/workspace",
  "version": "6.6.6",
  "description": "Malebolge engine",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "precommit": "lint-staged",
    "lint": "eslint --no-error-on-unmatched-pattern --fix . && prettier --write .",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/malebolge.git"
  },
  "keywords": [
    "malebolge",
    "engine"
  ],
  "author": "Salvatore Previti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/malebolge/issues"
  },
  "homepage": "https://github.com/SalvatorePreviti/malebolge#readme",
  "private": true,
  "engines": {
    "node": ">=18.17.0"
  },
  "prettier": "@balsamic/eslint-config/.prettierrc",
  "eslintConfig": {
    "$schema": "http://json.schemastore.org/prettierrc",
    "extends": "@balsamic"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mts,cts,cjs,mjs,json}": [
      "eslint --no-error-on-unmatched-pattern --fix",
      "prettier --write --log-level=warn"
    ],
    "*.{yml,yaml,md,htm,html,css,scss,less}": [
      "prettier --write --log-level=warn"
    ]
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@balsamic/eslint-config": "^0.6.7",
    "@balsamic/tsn": "^0.0.7",
    "@swc/core": "1.3.82",
    "@types/chai": "^4.3.6",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "chai": "^4.3.10",
    "eslint": "^8.50.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-vitest": "^0.3.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
